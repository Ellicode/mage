{
    "name": "mage",
    "version": "1.0.0",
    "main": "dist-electron/main/index.js",
    "description": "A raycast clone for Windows. A lightweight and fast launcher for Windows, built with Electron, Vite, and Vue 3.",
    "author": "Ellicode",
    "license": "MIT",
    "private": true,
    "keywords": [
        "electron",
        "rollup",
        "vite",
        "vue3",
        "vue"
    ],
    "debug": {
        "env": {
            "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
        }
    },
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vue-tsc --noEmit && vite build && electron-builder",
        "preview": "vite preview",
        "setup-apps": "node scripts/setup-apps.js",
        "install-apps": "node scripts/install-apps.js",
        "sync-deps": "node scripts/sync-deps.js",
        "build-apps": "npm run setup-apps && npm run install-apps && npm run build"
    },
    "devDependencies": {
        "@vitejs/plugin-vue": "^5.0.4",
        "electron": "^36.3.0",
        "electron-builder": "^24.13.3",
        "typescript": "^5.4.2"
        "vite": "^6.3.5",
        "vite-plugin-electron": "^0.28.4",
        "vite-plugin-electron-renderer": "^0.14.5",
        "vue": "^3.4.21",
        "vue-tsc": "^2.0.6"
    },
    "dependencies": {
        "@tailwindcss/vite": "^4.1.5",
        "geoip-lite": "^1.4.10",
        "get-installed-apps": "^1.1.1",
        "katex": "^0.16.22",
        "lucide-vue-next": "^0.508.0",
        "node-geoip": "^1.0.1",
        "regedit": "^5.1.4",
        "tailwindcss": "^4.1.5",
        "vatex": "^0.1.0",
        "vue-katex": "^0.5.0"
    }
}
